FROM golang:1.19

WORKDIR /apiserver/build

COPY . .

RUN go build -o server cmd/aifs-apiserver/apiserver.go

RUN mv /apiserver/build/server /apiserver/server

RUN rm -rf /apiserver/build

ENTRYPOINT [""]
